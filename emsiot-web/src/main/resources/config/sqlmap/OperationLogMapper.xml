<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ems.iot.manage.dao.OperationLogMapper">

    <insert id="insertOperationLog" parameterType="OperationLog">
        INSERT INTO operationlog(
		`requestname`,
		`adminname`,
		`requesturl`,
		`content`,
		`addtime`
		)
		VALUE 
		(
		#{requestname},
		#{adminname},
		#{requesturl},
		#{content},
		#{addtime}
		)
    </insert>    
    
    <select id="findOperationLogList" parameterType="map" resultType="OperationLog">
         SELECT * FROM operationlog
		 <where> 
			<if test="adminName != null">
		       `adminName` like "%"#{adminName}"%"
		    </if>
		 </where>
		 ORDER BY addtime DESC
    </select>
    
</mapper>